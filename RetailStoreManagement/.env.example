# Database Configuration
ConnectionStrings__DefaultConnection=Host=your-neon-host.neon.tech;Database=retail_store_db;Username=your-username;Password=your-password;SSL Mode=Require;Trust Server Certificate=true

# JWT Configuration
JwtSettings__SecretKey=your-super-secret-key-at-least-32-characters-long-for-production-use
JwtSettings__Issuer=store-management
JwtSettings__Audience=store-management
JwtSettings__ExpirationMinutes=1440

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning
Logging__LogLevel__Microsoft.EntityFrameworkCore=Warning

# CORS (comma-separated origins)
CORS__AllowedOrigins=http://localhost:3000,http://localhost:5173

# Application Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://localhost:5000;https://localhost:5001
